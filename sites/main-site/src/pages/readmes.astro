---
import { getEntry, render, getCollection } from 'astro:content';

import MarkdownTocLayout from '@layouts/MarkdownTocLayout.astro';
const collection = await getCollection('pipelines');
// get all id values in collection
console.log(collection.map((entry) => entry.id));

const entry = await getEntry('pipelines', 'methylseq-2.0.0-README.md');
const { Content, headings } = await render(entry);
---

<MarkdownTocLayout title={entry.id} description={entry.rendered} headings={headings}>
    <Content />
</MarkdownTocLayout>
